<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
    "http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>
    <package name="property" extends="default">
        <action name="*Property" method="{1}" class="propertyAction">
            <result name="list">/propertys/list.jsp</result>
            <result name="show">/propertys/show.jsp</result>
            <result name="input">/propertys/form.jsp</result>
            <result type="redirectAction">listProperty</result>
        </action>
    </package>
</struts>
