<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
    "http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>
    <package name="model" extends="default">
        <action name="*Model" method="{1}" class="modelAction">
            <result name="list">/models/list.jsp</result>
            <result name="show">/models/show.jsp</result>
            <result name="input">/models/form.jsp</result>
            <result type="redirectAction">listModel</result>
        </action>
    </package>
</struts>
