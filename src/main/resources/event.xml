<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
    "http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>
    <package name="event" extends="default">
        <action name="*Event" method="{1}" class="eventAction">
            <result name="list">/events/list.jsp</result>
            <result name="show">/events/show.jsp</result>
            <result name="input">/events/form.jsp</result>
            <result type="redirectAction">listEvent</result>
        </action>
    </package>
</struts>
