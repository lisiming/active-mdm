<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="tenggle.activemdm.event.model.Event" table="deviceevent">
        <id name="id">
            <generator class="increment" />
        </id>

        <property name="equipment_id" not-null="true" />
        <property name="devicemodel_id" not-null="true" />
        <property name="event_name" not-null="true" />
        <property name="event_category" not-null="true" />
        <property name="event_message" not-null="true" />
        <property name="create_time" not-null="true" />

    </class>
</hibernate-mapping>
